[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
[access "refs/for/refs/*"]
	read = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	push = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	label-Code-Review = -2..+2 group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	label-Verified = -1..+1 group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	rebase = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	removeReviewer = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	submit = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
[access "refs/heads/*"]
	read = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	create = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	push = +force group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	pushMerge = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
[access "refs/tags/*"]
	push = group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
	push = +force group user/MiguelNdeCarvalho (MiguelNdeCarvalho)
